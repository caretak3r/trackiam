{
  "Arn": "arn:aws:iam::aws:policy/AmazonEC2ContainerRegistryFullAccess",
  "CreateDate": "2015-12-21T17:06:48Z",
  "DefaultVersionId": "v3",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAIESRL7KD7IIVF6V4W",
  "PolicyName": "AmazonEC2ContainerRegistryFullAccess",
  "PolicyVersion": {
    "CreateDate": "2020-12-05T00:04:19Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "iam:CreateServiceLinkedRole"
          ],
          "Condition": {
            "StringEquals": {
              "iam:AWSServiceName": [
                "replication.ecr.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "cloudtrail:LookupEvents",
            "ecr:*"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v3"
  },
