{
  "Arn": "arn:aws:iam::aws:policy/service-role/ServerMigrationServiceLaunchRole",
  "CreateDate": "2018-11-26T19:53:06Z",
  "DefaultVersionId": "v3",
  "IsAttachable": true,
  "Path": "/service-role/",
  "PolicyId": "ANPAIIIAAMVUCBR2OLXZO",
  "PolicyName": "ServerMigrationServiceLaunchRole",
  "PolicyVersion": {
    "CreateDate": "2020-09-30T17:50:15Z",
    "Document": {
      "Statement": [
        {
          "Action": "iam:PassRole",
          "Condition": {
            "StringEquals": {
              "iam:PassedToService": "ec2.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ec2:AssociateIamInstanceProfile",
            "ec2:DisassociateIamInstanceProfile",
            "ec2:ReplaceIamInstanceProfileAssociation"
          ],
          "Condition": {
            "StringLike": {
              "ec2:ResourceTag/aws:cloudformation:stack-id": "arn:aws:cloudformation:*:*:stack/sms-app-*/*"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*:*:instance/*"
        },
        {
          "Action": [
            "ec2:ModifyInstanceAttribute",
            "ec2:StartInstances",
            "ec2:StopInstances",
            "ec2:TerminateInstances"
          ],
          "Condition": {
            "StringLike": {
              "ec2:ResourceTag/aws:cloudformation:stack-id": "arn:aws:cloudformation:*:*:stack/sms-app-*/*"
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": "ec2:CreateTags",
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*:*:instance/*"
        },
        {
          "Action": [
            "ec2:Describe*",
            "ec2:RunInstances"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v3"
  },
  "UpdateDate": "2020-09-30T17:50:15Z"
}
