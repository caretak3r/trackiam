{
  "Arn": "arn:aws:iam::aws:policy/AmazonHealthLakeFullAccess",
  "CreateDate": "2021-02-17T01:07:05Z",
  "DefaultVersionId": "v1",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4OMJS7NARX",
  "PolicyName": "AmazonHealthLakeFullAccess",
  "PolicyVersion": {
    "CreateDate": "2021-02-17T01:07:05Z",
    "Document": {
      "Statement": [
        {
          "Action": "iam:PassRole",
          "Condition": {
            "StringEquals": {
              "iam:PassedToService": "healthlake.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "healthlake:*",
            "iam:ListRoles",
            "s3:GetBucketLocation",
            "s3:ListAllMyBuckets",
            "s3:ListBucket"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v1"
  },
  "UpdateDate": "2021-02-17T01:07:05Z"
}
