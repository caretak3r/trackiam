{
  "Arn": "arn:aws:iam::aws:policy/AwsGlueDataBrewFullAccessPolicy",
  "CreateDate": "2020-11-11T16:51:39Z",
  "DefaultVersionId": "v7",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4ACNRIK7M3",
  "PolicyName": "AwsGlueDataBrewFullAccessPolicy",
  "PolicyVersion": {
    "CreateDate": "2021-11-18T19:37:52Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "iam:PassRole"
          ],
          "Condition": {
            "StringEquals": {
              "iam:PassedToService": [
                "databrew.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:iam::*:role/*"
        },
        {
          "Action": [
            "kms:GenerateDataKey"
          ],
          "Condition": {
            "StringLike": {
              "kms:ViaService": "s3.*.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "*"
          ]
        },
        {
          "Action": [
            "secretsmanager:CreateSecret"
          ],
          "Condition": {
            "ForAnyValue:StringEquals": {
              "aws:CalledVia": [
                "databrew.amazonaws.com"
              ]
            },
            "StringLike": {
              "secretsmanager:Name": "databrew!default"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:secretsmanager:*:*:secret:databrew!default-*"
        },
        {
          "Action": [
            "secretsmanager:GetSecretValue"
          ],
          "Condition": {
            "ForAnyValue:StringEquals": {
              "aws:CalledVia": [
                "databrew.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:secretsmanager:*:*:secret:databrew!default-*"
        },
        {
          "Action": [
            "appflow:DescribeFlow",
            "appflow:DescribeFlowExecutionRecords",
            "appflow:ListFlows",
            "cloudtrail:LookupEvents",
            "databrew:*",
            "dataexchange:CreateJob",
            "dataexchange:GetJob",
            "dataexchange:ListDataSetRevisions",
            "dataexchange:ListDataSets",
            "dataexchange:ListRevisionAssets",
            "dataexchange:StartJob",
            "ec2:DescribeSecurityGroups",
            "ec2:DescribeSubnets",
            "ec2:DescribeVpcs",
            "glue:GetConnection",
            "glue:GetConnections",
            "glue:GetDataCatalogEncryptionSettings",
            "glue:GetDatabases",
            "glue:GetPartitions",
            "glue:GetTable",
            "glue:GetTables",
            "iam:GetRole",
            "iam:ListRoles",
            "kms:DescribeKey",
            "kms:ListAliases",
            "kms:ListKeys",
            "redshift-data:ListDatabases",
            "redshift-data:ListSchemas",
            "redshift-data:ListTables",
            "redshift:DescribeClusterSubnetGroups",
            "redshift:DescribeClusters",
            "s3:GetBucketCORS",
            "s3:GetBucketLocation",
            "s3:GetEncryptionConfiguration",
            "s3:ListAllMyBuckets",
            "secretsmanager:DescribeSecret",
            "secretsmanager:ListSecrets",
            "sts:GetCallerIdentity"
          ],
          "Effect": "Allow",
          "Resource": [
            "*"
          ]
        },
        {
          "Action": [
            "glue:CreateConnection"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:glue:*:*:catalog",
            "arn:aws:glue:*:*:connection/AwsGlueDataBrew-*"
          ]
        },
        {
          "Action": [
            "glue:CreateTable"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:glue:*:*:catalog",
            "arn:aws:glue:*:*:database/*",
            "arn:aws:glue:*:*:table/*/awsgluedatabrew*"
          ]
        },
        {
          "Action": [
            "glue:GetDatabases"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:glue:*:*:catalog",
            "arn:aws:glue:*:*:database/*"
          ]
        },
        {
          "Action": [
            "kms:GenerateRandom"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "s3:GetObject",
            "s3:ListBucket"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:s3:::databrew-public-datasets-*"
          ]
        },
        {
          "Action": [
            "secretsmanager:CreateSecret"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:secretsmanager:*:*:secret:AwsGlueDataBrew-*"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v7"
  },
  "UpdateDate": "2021-11-18T19:37:52Z"
}
