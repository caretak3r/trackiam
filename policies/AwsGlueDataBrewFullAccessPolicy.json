{
  "Arn": "arn:aws:iam::aws:policy/AwsGlueDataBrewFullAccessPolicy",
  "CreateDate": "2020-11-11T16:51:39Z",
  "DefaultVersionId": "v2",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4ACNRIK7M3",
  "PolicyName": "AwsGlueDataBrewFullAccessPolicy",
  "PolicyVersion": {
    "CreateDate": "2020-11-12T23:04:55Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "iam:PassRole"
          ],
          "Condition": {
            "StringEquals": {
              "iam:PassedToService": [
                "databrew.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:iam::*:role/*"
        },
        {
          "Action": [
            "kms:GenerateDataKey"
          ],
          "Condition": {
            "StringLike": {
              "kms:ViaService": "s3.*.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "*"
          ]
        },
        {
          "Action": [
            "cloudtrail:LookupEvents",
            "databrew:*",
            "dataexchange:CreateJob",
            "dataexchange:GetJob",
            "dataexchange:ListDataSetRevisions",
            "dataexchange:ListDataSets",
            "dataexchange:ListRevisionAssets",
            "dataexchange:StartJob",
            "glue:GetDataCatalogEncryptionSettings",
            "glue:GetDatabases",
            "glue:GetPartitions",
            "glue:GetTable",
            "glue:GetTables",
            "iam:GetRole",
            "iam:ListRoles",
            "kms:DescribeKey",
            "kms:ListAliases",
            "kms:ListKeys",
            "s3:GetBucketCORS",
            "s3:GetBucketLocation",
            "s3:GetEncryptionConfiguration",
            "s3:ListAllMyBuckets",
            "sts:GetCallerIdentity"
          ],
          "Effect": "Allow",
          "Resource": [
            "*"
          ]
        },
        {
          "Action": [
            "s3:GetObject",
            "s3:ListBucket"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:s3:::databrew-public-datasets-*"
          ]
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v2"
  },
  "UpdateDate": "2020-11-12T23:04:55Z"
}
