{
  "Arn": "arn:aws:iam::aws:policy/AmazonEC2RolePolicyForLaunchWizard",
  "CreateDate": "2019-11-13T08:05:53Z",
  "DefaultVersionId": "v7",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4CBGI56NFS",
  "PolicyName": "AmazonEC2RolePolicyForLaunchWizard",
  "PolicyVersion": {
    "CreateDate": "2020-10-09T22:28:01Z",
    "Document": {
      "Statement": [
        {
          "Action": "ssm:SendCommand",
          "Condition": {
            "StringLike": {
              "ssm:resourceTag/LaunchWizardApplicationType": "*"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*:*:instance/*"
        },
        {
          "Action": [
            "cloudformation:DescribeStackResource",
            "cloudformation:DescribeStackResources",
            "cloudformation:DescribeStacks",
            "cloudformation:SignalResource",
            "ec2:Describe*"
          ],
          "Condition": {
            "ForAllValues:StringEquals": {
              "aws:TagKeys": "LaunchWizardResourceGroupID"
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ec2:AttachVolume",
            "ec2:RebootInstances",
            "ec2:StartInstances",
            "ec2:StopInstances"
          ],
          "Condition": {
            "StringLike": {
              "ec2:ResourceTag/LaunchWizardResourceGroupID": "*"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:instance/*",
            "arn:aws:ec2:*:*:volume/*"
          ]
        },
        {
          "Action": [
            "ec2:CreateTags",
            "ec2:CreateVolume"
          ],
          "Condition": {
            "ForAllValues:StringEquals": {
              "aws:TagKeys": [
                "LaunchWizardApplicationType",
                "LaunchWizardResourceGroupID"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ec2:ReplaceRoute"
          ],
          "Condition": {
            "StringLike": {
              "ec2:ResourceTag/LaunchWizardApplicationType": "*"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:ec2:*:*:route-table/*"
        },
        {
          "Action": "logs:Create*",
          "Effect": "Allow",
          "Resource": "arn:aws:logs:*:*:*"
        },
        {
          "Action": [
            "cloudwatch:GetMetricStatistics",
            "cloudwatch:PutMetricData",
            "ec2:AssociateAddress",
            "ec2:DescribeAddresses",
            "ec2:DescribeImages",
            "ec2:DescribeInstances",
            "ec2:DescribeRegions",
            "ec2:DescribeRouteTables",
            "ec2:DescribeVolumes",
            "ec2:ModifyInstanceAttribute",
            "ssm:GetCommandInvocation"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "dynamodb:BatchGetItem",
            "dynamodb:CreateTable",
            "dynamodb:DeleteTable",
            "dynamodb:DescribeTable",
            "dynamodb:PutItem",
            "dynamodb:Query",
            "dynamodb:Scan",
            "dynamodb:UpdateItem",
            "dynamodb:UpdateTable",
            "s3:GetBucketLocation",
            "s3:GetObject",
            "s3:ListBucket",
            "sqs:ReceiveMessage",
            "sqs:SendMessage"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:dynamodb:*:*:table/LaunchWizard*",
            "arn:aws:s3:::launchwizard*",
            "arn:aws:sqs:*:*:LaunchWizard*"
          ]
        },
        {
          "Action": [
            "logs:DescribeLogGroups",
            "logs:DescribeLogStreams",
            "logs:PutLogEvents",
            "s3:GetBucketLocation",
            "s3:GetObject",
            "s3:ListBucket",
            "s3:PutObject",
            "s3:PutObjectTagging"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:logs:*:*:*",
            "arn:aws:s3:::aws-sap-data-provider/config.properties",
            "arn:aws:s3:::launchwizard*"
          ]
        },
        {
          "Action": [
            "ssm:GetDocument",
            "ssm:SendCommand"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ssm:*:*:document/AWSSAP-InstallBackint"
          ]
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v7"
  },
  "UpdateDate": "2020-10-09T22:28:01Z"
}
