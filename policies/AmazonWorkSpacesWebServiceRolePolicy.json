{
  "Arn": "arn:aws:iam::aws:policy/aws-service-role/AmazonWorkSpacesWebServiceRolePolicy",
  "CreateDate": "2021-11-30T13:15:53Z",
  "DefaultVersionId": "v1",
  "IsAttachable": true,
  "Path": "/aws-service-role/",
  "PolicyId": "ANPAZKAPJZG4JW4G4DATC",
  "PolicyName": "AmazonWorkSpacesWebServiceRolePolicy",
  "PolicyVersion": {
    "CreateDate": "2021-11-30T13:15:53Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "cloudwatch:PutMetricData"
          ],
          "Condition": {
            "StringEquals": {
              "cloudwatch:namespace": "AWS/WorkSpacesWeb"
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ec2:AssociateAddress",
            "ec2:CreateNetworkInterface",
            "ec2:DeleteNetworkInterface",
            "ec2:DescribeAvailabilityZones",
            "ec2:DescribeNetworkInterfaces",
            "ec2:DescribeRouteTables",
            "ec2:DescribeSecurityGroups",
            "ec2:DescribeSubnets",
            "ec2:DescribeVpcEndpoints",
            "ec2:DescribeVpcs",
            "ec2:DisassociateAddress"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v1"
  },
  "UpdateDate": "2021-11-30T13:15:53Z"
}
