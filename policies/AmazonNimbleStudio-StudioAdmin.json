{
  "Arn": "arn:aws:iam::aws:policy/AmazonNimbleStudio-StudioAdmin",
  "CreateDate": "2021-04-28T04:47:36Z",
  "DefaultVersionId": "v2",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4PTQDL2ND4",
  "PolicyName": "AmazonNimbleStudio-StudioAdmin",
  "PolicyVersion": {
