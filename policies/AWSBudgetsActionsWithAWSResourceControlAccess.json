{
  "Arn": "arn:aws:iam::aws:policy/AWSBudgetsActionsWithAWSResourceControlAccess",
  "CreateDate": "2020-10-15T17:19:12Z",
  "DefaultVersionId": "v1",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4AHTKKGHHS",
  "PolicyName": "AWSBudgetsActionsWithAWSResourceControlAccess",
  "PolicyVersion": {
    "CreateDate": "2020-10-15T17:19:12Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "iam:PassRole"
          ],
          "Condition": {
            "StringEquals": {
              "iam:PassedToService": "budgets.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "aws-portal:ModifyBilling",
            "ec2:DescribeInstances",
            "iam:ListGroups",
            "iam:ListPolicies",
            "iam:ListRoles",
            "iam:ListUsers",
            "organizations:ListAccounts",
            "organizations:ListOrganizationalUnitsForParent",
            "organizations:ListPolicies",
            "organizations:ListRoots",
            "rds:DescribeDBInstances",
            "sns:ListTopics"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "aws-portal:ViewBilling"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "budgets:*"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v1"
  },
  "UpdateDate": "2020-10-15T17:19:12Z"
}
