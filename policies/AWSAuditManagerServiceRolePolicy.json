          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "OrganizationsAccess"
        },
        {
          "Action": [
            "route53:GetQueryLoggingConfig"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "Route53Access"
        },
        {
          "Action": [
            "s3:GetLifecycleConfiguration"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "S3Access"
        },
        {
          "Action": [
            "securityhub:DescribeStandards"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "SecurityHubAccess"
        },
        {
          "Action": [
            "waf:ListActivatedRulesInRuleGroup"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "WAFAccess"
        }
      ],
      "Version": "2012-10-17"
