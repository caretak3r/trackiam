{
  "Arn": "arn:aws:iam::aws:policy/AmazonMacieFullAccess",
  "CreateDate": "2017-08-14T14:54:30Z",
  "DefaultVersionId": "v4",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAJJF2N5FR6S5TZN5OA",
  "PolicyName": "AmazonMacieFullAccess",
  "PolicyVersion": {
    "CreateDate": "2022-03-07T18:08:14Z",
    "Document": {
      "Statement": [
        {
          "Action": "iam:CreateServiceLinkedRole",
          "Condition": {
            "StringLike": {
              "iam:AWSServiceName": "macie.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:iam::*:role/AWSServiceRoleForAmazonMacie"
        },
        {
          "Action": "pricing:GetProducts",
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "macie2:*"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v4"
  },
  "UpdateDate": "2022-03-07T18:08:14Z"
}
