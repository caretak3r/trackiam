{
  "Arn": "arn:aws:iam::aws:policy/AmazonLookoutEquipmentFullAccess",
  "CreateDate": "2021-04-08T15:52:08Z",
  "DefaultVersionId": "v1",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4KPPCPGNJA",
  "PolicyName": "AmazonLookoutEquipmentFullAccess",
  "PolicyVersion": {
    "CreateDate": "2021-04-08T15:52:08Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "iam:PassRole"
          ],
          "Condition": {
            "StringEquals": {
              "iam:PassedToService": [
                "lookoutequipment.amazonaws.com"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "kms:CreateGrant",
            "kms:DescribeKey",
            "kms:RetireGrant"
          ],
          "Condition": {
            "StringLike": {
              "kms:ViaService": "lookoutequipment.*.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "lookoutequipment:*"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v1"
  },
