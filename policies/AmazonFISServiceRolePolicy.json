{
  "Arn": "arn:aws:iam::aws:policy/aws-service-role/AmazonFISServiceRolePolicy",
  "CreateDate": "2020-12-21T21:18:19Z",
  "DefaultVersionId": "v4",
  "IsAttachable": true,
  "Path": "/aws-service-role/",
  "PolicyId": "ANPAZKAPJZG4JLZR2TQJD",
  "PolicyName": "AmazonFISServiceRolePolicy",
  "PolicyVersion": {
    "CreateDate": "2022-01-06T16:43:13Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "events:DeleteRule",
            "events:PutRule",
            "events:PutTargets",
            "events:RemoveTargets"
          ],
          "Condition": {
            "StringEquals": {
              "events:ManagedBy": "fis.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "EventBridge"
        },
        {
          "Action": [
            "cloudwatch:DescribeAlarmHistory",
            "cloudwatch:DescribeAlarms"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "CloudWatch"
        },
        {
          "Action": [
            "ec2:DescribeInstances",
            "ecs:DescribeClusters",
            "eks:DescribeNodegroup",
            "iam:GetRole",
            "iam:GetUser",
            "iam:ListRoles",
            "iam:ListUsers",
            "rds:DescribeDBClusters",
            "rds:DescribeDBInstances"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "DescribeUserResources"
        },
        {
          "Action": [
            "events:DescribeRule"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "EventBridgeDescribe"
        },
        {
          "Action": [
            "tag:GetResources"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "Tagging"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v4"
  },
  "UpdateDate": "2022-01-06T16:43:13Z"
}
