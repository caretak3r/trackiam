{
  "Arn": "arn:aws:iam::aws:policy/service-role/ServerMigrationServiceRole",
  "CreateDate": "2016-10-24T21:19:00Z",
  "DefaultVersionId": "v7",
  "IsAttachable": true,
  "Path": "/service-role/",
  "PolicyId": "ANPAJMBH3M6BO63XFW2D4",
  "PolicyName": "ServerMigrationServiceRole",
  "PolicyVersion": {
    "CreateDate": "2020-08-01T03:40:03Z",
    "Document": {
      "Statement": [
        {
          "Action": "iam:PassRole",
          "Condition": {
            "StringLike": {
              "iam:AssociatedResourceArn": "arn:aws:cloudformation:*:*:stack/sms-app-*/*"
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "cloudformation:CreateChangeSet",
            "cloudformation:CreateStack",
            "cloudformation:DeleteStack",
            "cloudformation:ExecuteChangeSet"
          ],
          "Condition": {
            "ForAllValues:StringLikeIfExists": {
              "cloudformation:ResourceTypes": [
                "AWS::EC2::*"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:cloudformation:*:*:stack/sms-app-*/*"
        },
        {
          "Action": "iam:GetInstanceProfile",
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": "iam:GetRole",
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": "ssm:SendCommand",
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:instance/*",
            "arn:aws:s3:::*",
            "arn:aws:ssm:*:*:document/AWS-RunPowerShellScript",
            "arn:aws:ssm:*:*:document/AWS-RunRemoteScript",
            "arn:aws:ssm:*:*:document/AWS-RunShellScript",
            "arn:aws:ssm:*:*:managed-instance/*"
          ]
        },
        {
          "Action": [
            "cloudformation:DeleteChangeSet",
            "cloudformation:DescribeChangeSet",
            "cloudformation:DescribeStackEvents",
            "cloudformation:DescribeStackResources",
            "cloudformation:GetTemplate"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:cloudformation:*:*:stack/sms-app-*/*"
        },
        {
          "Action": [
            "cloudformation:DescribeStackResource",
            "cloudformation:DescribeStacks",
            "cloudformation:ValidateTemplate",
            "s3:ListAllMyBuckets"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ec2:CopyImage",
            "ec2:CopySnapshot",
            "ec2:CreateTags",
            "ec2:DeleteSnapshot",
            "ec2:DeleteTags",
            "ec2:DeregisterImage",
            "ec2:Describe*",
            "ec2:DescribeImportImageTasks",
            "ec2:GetEbsEncryptionByDefault",
            "ec2:ImportImage",
            "ec2:ModifySnapshotAttribute"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "s3:CreateBucket",
            "s3:DeleteBucket",
            "s3:DeleteObject",
            "s3:GetBucketAcl",
            "s3:GetBucketLocation",
            "s3:GetObject",
            "s3:ListAllMyBuckets",
            "s3:ListBucket",
            "s3:PutLifecycleConfiguration",
            "s3:PutObject",
            "s3:PutObjectAcl"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:s3:::sms-app-*"
        },
        {
          "Action": [
            "sms:CreateReplicationJob",
            "sms:DeleteReplicationJob",
            "sms:GetReplicationJobs",
            "sms:GetReplicationRuns",
            "sms:GetServers",
            "sms:ImportServerCatalog",
            "sms:StartOnDemandReplicationRun",
            "sms:UpdateReplicationJob"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ssm:CancelCommand",
            "ssm:GetCommandInvocation"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v7"
  },
  "UpdateDate": "2020-08-01T03:40:03Z"
}
