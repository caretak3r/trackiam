{
  "Arn": "arn:aws:iam::aws:policy/aws-service-role/AmazonRedshiftServiceLinkedRolePolicy",
  "CreateDate": "2017-09-18T19:19:45Z",
  "DefaultVersionId": "v4",
  "IsAttachable": true,
  "Path": "/aws-service-role/",
  "PolicyId": "ANPAJPY2VXNRUYOY3SRZS",
  "PolicyName": "AmazonRedshiftServiceLinkedRolePolicy",
  "PolicyVersion": {
    "CreateDate": "2021-11-15T06:41:10Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "ec2:AssociateAddress",
            "ec2:CreateNetworkInterface",
            "ec2:CreateVpcEndpoint",
            "ec2:DeleteNetworkInterface",
            "ec2:DeleteVpcEndpoints",
            "ec2:DescribeAddresses",
            "ec2:DescribeNetworkInterfaces",
            "ec2:DescribeSubnets",
            "ec2:DescribeVpcEndpoints",
            "ec2:DescribeVpcs",
            "ec2:DisassociateAddress",
            "ec2:ModifyNetworkInterfaceAttribute",
            "ec2:ModifyVpcEndpoint"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "logs:CreateLogGroup",
            "logs:PutRetentionPolicy"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:logs:*:*:log-group:/aws/redshift/*"
          ],
          "Sid": "EnableCreationAndManagementOfRedshiftCloudwatchLogGroups"
        },
        {
          "Action": [
            "logs:CreateLogStream",
            "logs:DescribeLogStreams",
            "logs:GetLogEvents",
            "logs:PutLogEvents"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:logs:*:*:log-group:/aws/redshift/*:log-stream:*"
          ],
          "Sid": "EnableCreationAndManagementOfRedshiftCloudwatchLogStreams"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v4"
  },
  "UpdateDate": "2021-11-15T06:41:10Z"
}
