{
  "Arn": "arn:aws:iam::aws:policy/CloudFrontFullAccess",
  "CreateDate": "2015-02-06T18:39:50Z",
  "DefaultVersionId": "v5",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAIPRV52SH6HDCCFY6U",
  "PolicyName": "CloudFrontFullAccess",
  "PolicyVersion": {
    "CreateDate": "2020-08-27T17:09:55Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "acm:ListCertificates",
            "cloudfront:*",
            "iam:ListServerCertificates",
            "waf:GetWebACL",
            "waf:ListWebACLs",
            "wafv2:GetWebACL",
            "wafv2:ListWebACLs"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "iam:ListRoles"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:iam:::*"
        },
        {
          "Action": [
            "kinesis:DescribeStream",
            "kinesis:ListStreams"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:kinesis:::*"
        },
        {
          "Action": [
            "s3:ListAllMyBuckets"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:s3:::*"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v5"
  },
  "UpdateDate": "2020-08-27T17:09:55Z"
}
