{
  "Arn": "arn:aws:iam::aws:policy/CloudFrontFullAccess",
  "CreateDate": "2015-02-06T18:39:50Z",
  "DefaultVersionId": "v6",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAIPRV52SH6HDCCFY6U",
  "PolicyName": "CloudFrontFullAccess",
  "PolicyVersion": {
    "CreateDate": "2020-09-03T20:18:42Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "acm:ListCertificates",
            "cloudfront:*",
            "iam:ListServerCertificates",
            "kinesis:ListStreams",
            "waf:GetWebACL",
            "waf:ListWebACLs",
            "wafv2:GetWebACL",
            "wafv2:ListWebACLs"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "iam:ListRoles"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:iam::*:*"
        },
        {
          "Action": [
            "kinesis:DescribeStream"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:kinesis:*:*:*"
        },
        {
          "Action": [
            "s3:ListAllMyBuckets"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:s3:::*"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v6"
  },
  "UpdateDate": "2020-09-03T20:18:42Z"
}
