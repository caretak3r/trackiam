          "Resource": "*"
        },
        {
          "Action": [
            "mgn:SendAgentLogsForMgn",
            "mgn:SendAgentMetricsForMgn",
            "mgn:SendClientLogsForMgn"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
