{
  "Arn": "arn:aws:iam::aws:policy/service-role/AWSIoTFleetHubFederationAccess",
  "CreateDate": "2020-12-15T08:08:05Z",
  "DefaultVersionId": "v4",
  "IsAttachable": true,
  "Path": "/service-role/",
  "PolicyId": "ANPAZKAPJZG4H4EGQA254",
  "PolicyName": "AWSIoTFleetHubFederationAccess",
  "PolicyVersion": {
    "CreateDate": "2021-11-15T23:28:19Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "cloudwatch:DeleteAlarms",
            "cloudwatch:DescribeAlarmHistory",
            "cloudwatch:PutMetricAlarm"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:cloudwatch:*:*:iotfleethub*"
        },
        {
          "Action": [
            "cloudwatch:DescribeAlarms",
            "cloudwatch:GetMetricData",
            "cloudwatch:ListMetrics",
            "iot:CancelJob",
            "iot:CancelJobExecution",
            "iot:CreateFleetMetric",
            "iot:CreateJob",
            "iot:DeleteFleetMetric",
            "iot:DescribeEndpoint",
            "iot:DescribeFleetMetric",
            "iot:DescribeIndex",
            "iot:DescribeJob",
            "iot:DescribeJobExecution",
            "iot:DescribeJobTemplate",
            "iot:DescribeSecurityProfile",
            "iot:DescribeThingGroup",
            "iot:GetBucketsAggregation",
            "iot:GetCardinality",
            "iot:GetIndexingConfiguration",
            "iot:GetPercentiles",
            "iot:GetStatistics",
            "iot:GetThingShadow",
            "iot:ListActiveViolations",
            "iot:ListFleetMetrics",
            "iot:ListJobExecutionsForJob",
            "iot:ListJobExecutionsForThing",
            "iot:ListJobTemplates",
            "iot:ListJobs",
            "iot:ListNamedShadowsForThing",
            "iot:ListSecurityProfiles",
            "iot:ListThingGroups",
            "iot:ListThingsInThingGroup",
            "iot:SearchIndex",
            "iot:UpdateFleetMetric",
            "iotfleethub:DescribeApplication",
            "sns:ListTopics"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "sns:CreateTopic",
            "sns:DeleteTopic",
            "sns:ListSubscriptionsByTopic",
            "sns:Subscribe",
            "sns:Unsubscribe"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:sns:*:*:iotfleethub*"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v4"
  },
  "UpdateDate": "2021-11-15T23:28:19Z"
