{
  "Arn": "arn:aws:iam::aws:policy/AmazonSageMakerGroundTruthExecution",
  "CreateDate": "2020-07-09T19:30:20Z",
  "DefaultVersionId": "v2",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4FYNFSJXO3",
  "PolicyName": "AmazonSageMakerGroundTruthExecution",
  "PolicyVersion": {
    "CreateDate": "2022-04-11T21:22:37Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "s3:GetObject"
          ],
          "Condition": {
            "StringEqualsIgnoreCase": {
              "s3:ExistingObjectTag/SageMaker": "true"
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": "sns:Subscribe",
          "Condition": {
            "StringEquals": {
              "sns:Protocol": "sqs"
            },
            "StringLike": {
              "sns:Endpoint": "arn:aws:sqs:*:*:*GroundTruth*"
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:sns:*:*:*GroundTruth*",
            "arn:aws:sns:*:*:*Groundtruth*",
            "arn:aws:sns:*:*:*SageMaker*",
            "arn:aws:sns:*:*:*Sagemaker*",
            "arn:aws:sns:*:*:*groundTruth*",
            "arn:aws:sns:*:*:*groundtruth*",
            "arn:aws:sns:*:*:*sageMaker*",
            "arn:aws:sns:*:*:*sagemaker*"
          ],
          "Sid": "StreamingTopicSubscribe"
        },
        {
          "Action": [
            "s3:AbortMultipartUpload",
            "s3:GetObject",
            "s3:PutObject"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:s3:::*GroundTruth*",
            "arn:aws:s3:::*Groundtruth*",
            "arn:aws:s3:::*SageMaker*",
            "arn:aws:s3:::*Sagemaker*",
            "arn:aws:s3:::*groundtruth*",
            "arn:aws:s3:::*sagemaker*"
          ]
        },
        {
          "Action": [
            "s3:GetBucketLocation",
            "s3:ListBucket"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "cloudwatch:PutMetricData",
            "logs:CreateLogGroup",
            "logs:CreateLogStream",
            "logs:DescribeLogStreams",
            "logs:PutLogEvents"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "CloudWatch"
        },
        {
          "Action": [
            "lambda:InvokeFunction"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:lambda:*:*:function:*GtRecipe*",
            "arn:aws:lambda:*:*:function:*LabelingFunction*",
            "arn:aws:lambda:*:*:function:*SageMaker*",
            "arn:aws:lambda:*:*:function:*Sagemaker*",
            "arn:aws:lambda:*:*:function:*sagemaker*"
          ],
          "Sid": "CustomLabelingJobs"
        },
        {
          "Action": [
            "sns:Publish"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:sns:*:*:*GroundTruth*",
            "arn:aws:sns:*:*:*Groundtruth*",
            "arn:aws:sns:*:*:*SageMaker*",
            "arn:aws:sns:*:*:*Sagemaker*",
            "arn:aws:sns:*:*:*groundTruth*",
            "arn:aws:sns:*:*:*groundtruth*",
            "arn:aws:sns:*:*:*sageMaker*",
            "arn:aws:sns:*:*:*sagemaker*"
          ],
          "Sid": "StreamingTopic"
        },
        {
          "Action": [
            "sns:Unsubscribe"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "StreamingTopicUnsubscribe"
        },
        {
          "Action": [
            "sqs:CreateQueue",
            "sqs:DeleteMessage",
            "sqs:GetQueueAttributes",
            "sqs:GetQueueUrl",
            "sqs:ReceiveMessage",
            "sqs:SendMessage",
            "sqs:SetQueueAttributes"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:sqs:*:*:*GroundTruth*",
          "Sid": "StreamingQueue"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v2"
  },
  "UpdateDate": "2022-04-11T21:22:37Z"
}
