{
  "Arn": "arn:aws:iam::aws:policy/AWSWAFConsoleFullAccess",
  "CreateDate": "2020-04-06T18:38:38Z",
  "DefaultVersionId": "v3",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4AZOTQ7KAT",
  "PolicyName": "AWSWAFConsoleFullAccess",
  "PolicyVersion": {
    "CreateDate": "2021-11-15T19:33:52Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "apigateway:GET",
            "apigateway:SetWebACL",
            "appsync:ListGraphqlApis",
            "appsync:SetWebACL",
            "cloudfront:ListDistributions",
            "cloudfront:ListDistributionsByWebACLId",
            "cloudfront:UpdateDistribution",
            "cloudwatch:GetMetricData",
            "cloudwatch:GetMetricStatistics",
            "cloudwatch:ListMetrics",
            "ec2:DescribeRegions",
            "elasticloadbalancing:DescribeLoadBalancers",
            "elasticloadbalancing:SetWebACL",
            "waf-regional:*",
            "waf:*",
            "wafv2:*"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "AllowUseOfAWSWAF"
        },
        {
          "Action": [
            "logs:CreateLogDelivery",
            "logs:DeleteLogDelivery"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:logs:*:*:aws-waf-logs-*",
            "arn:aws:s3:::aws-waf-logs-*"
          ],
          "Sid": "AllowLogDeliverySubscription"
        },
        {
          "Action": [
            "logs:DescribeLogGroups",
            "logs:DescribeResourcePolicies",
            "logs:PutResourcePolicy"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:logs:*:*:aws-waf-logs-*"
          ],
          "Sid": "GrantLogDeliveryPermissionForCloudWatchLogGroup"
        },
        {
          "Action": [
            "s3:GetBucketPolicy",
            "s3:PutBucketPolicy"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:s3:::aws-waf-logs-*"
          ],
          "Sid": "GrantLogDeliveryPermissionForS3Bucket"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v3"
  },
  "UpdateDate": "2021-11-15T19:33:52Z"
}
