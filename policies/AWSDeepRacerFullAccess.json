    "Document": {
      "Statement": [
        {
          "Action": [
            "s3:DeleteObject",
            "s3:DeleteObjectVersion",
            "s3:GetBucketAcl",
            "s3:GetBucketLocation",
            "s3:GetBucketPolicy",
            "s3:GetObject",
            "s3:GetObjectAcl",
            "s3:GetObjectVersion",
            "s3:ListBucket",
            "s3:PutBucketPolicy"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:s3:::*DeepRacer*",
            "arn:aws:s3:::*DeepRacer*/*",
            "arn:aws:s3:::*Deepracer*",
            "arn:aws:s3:::*Deepracer*/*",
            "arn:aws:s3:::*deepracer*",
            "arn:aws:s3:::*deepracer*/*",
            "arn:aws:s3:::dr-*",
            "arn:aws:s3:::dr-*/*"
          ]
        },
        {
          "Action": [
            "s3:ListAllMyBuckets"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v1"
  },
  "UpdateDate": "2020-10-05T22:03:10Z"
