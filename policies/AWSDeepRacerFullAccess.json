{
  "Arn": "arn:aws:iam::aws:policy/AWSDeepRacerFullAccess",
  "CreateDate": "2020-10-05T22:03:10Z",
  "DefaultVersionId": "v1",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4JFTOPTVBM",
  "PolicyName": "AWSDeepRacerFullAccess",
  "PolicyVersion": {
