{
  "Arn": "arn:aws:iam::aws:policy/AmazonAppFlowFullAccess",
  "CreateDate": "2020-06-02T23:30:14Z",
  "DefaultVersionId": "v2",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4PGBU2ALC4",
  "PolicyName": "AmazonAppFlowFullAccess",
  "PolicyVersion": {
    "CreateDate": "2020-12-07T22:49:15Z",
    "Document": {
      "Statement": [
        {
          "Action": "secretsmanager:CreateSecret",
          "Condition": {
            "ForAnyValue:StringEquals": {
              "aws:CalledVia": [
                "appflow.amazonaws.com"
              ]
            },
            "StringLike": {
              "secretsmanager:Name": "appflow!*"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "SecretsManagerCreateSecretAccess"
        },
        {
          "Action": [
            "kms:CreateGrant"
          ],
          "Condition": {
            "Bool": {
              "kms:GrantIsForAWSResource": "true"
            },
            "StringLike": {
              "kms:ViaService": "appflow.*.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "KMSGrantAccess"
        },
        {
          "Action": [
            "kms:ListGrants"
          ],
          "Condition": {
            "StringLike": {
              "kms:ViaService": "appflow.*.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "KMSListGrantAccess"
        },
        {
          "Action": [
            "secretsmanager:PutResourcePolicy"
          ],
          "Condition": {
            "ForAnyValue:StringEquals": {
              "aws:CalledVia": [
                "appflow.amazonaws.com"
              ]
            },
            "StringEqualsIgnoreCase": {
              "secretsmanager:ResourceTag/aws:secretsmanager:owningService": "appflow"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "SecretsManagerPutResourcePolicyAccess"
        },
        {
          "Action": "appflow:*",
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": "iam:ListRoles",
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "ListRolesForRedshift"
        },
        {
          "Action": [
            "kms:DescribeKey",
            "kms:ListAliases",
            "kms:ListKeys"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "KMSListAccess"
        },
        {
          "Action": [
            "s3:GetBucketLocation",
            "s3:GetBucketPolicy",
            "s3:ListAllMyBuckets",
            "s3:ListBucket"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "S3ReadAccess"
        },
        {
          "Action": [
            "s3:PutBucketPolicy"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:s3:::appflow-*",
          "Sid": "S3PutBucketPolicyAccess"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v2"
  },
  "UpdateDate": "2020-12-07T22:49:15Z"
}
